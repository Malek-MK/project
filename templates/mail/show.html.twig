{% extends 'template1.html.twig' %}

{% block title %}Mail{% endblock %}

{% block body %}
    <h1>Mail</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mail.id }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ mail.subject }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ mail.mail }}</td>
            </tr>
            <tr>
                <th>Object</th>
                <td>{{ mail.object }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('mail_index') }}">back to list</a>

    

    
{% endblock %}
